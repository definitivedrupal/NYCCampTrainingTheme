/**
 * Implements hook_preprocess_page().
 */
function twigtastic_preprocess_page(&$variables) {
  $fit_text = array(
    '#attached' => array(
      'library' => array(
        'twigtastic/fittext',
      ),
    ),
  );
  drupal_render($fit_text);
}
